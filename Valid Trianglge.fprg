    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a" type="Real" array="False" size=""/>
            <declare name="b" type="Real" array="False" size=""/>
            <declare name="c" type="Real" array="False" size=""/>
            <declare name="stp" type="Integer" array="False" size=""/>
            <assign variable="stp" expression="10"/>
            <while expression="stp != 1">
                <output expression="&quot;Tell the length of 1st side : &quot;" newline="True"/>
                <input variable="a"/>
                <output expression="&quot;Tell the length of 2nd side : &quot;" newline="True"/>
                <input variable="b"/>
                <output expression="&quot;Tell the length of 3rd side : &quot;" newline="True"/>
                <input variable="c"/>
                <if expression="a+b &gt; c and c+b &gt; a and a+c &gt; b">
                    <then>
                        <output expression="&quot;This is a valid triangle and it can be formed &quot;" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;This is not a valid triangle and it can't be formed &quot;" newline="True"/>
                    </else>
                </if>
                <output expression="&quot;Press 1 to stop : &quot;" newline="True"/>
                <input variable="stp"/>
            </while>
        </body>
    </function>
</flowgorithm>
